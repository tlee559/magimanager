{
  "name": "magimanager",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "db:generate": "npm run generate --workspace=@magimanager/database",
    "db:push": "npm run push --workspace=@magimanager/database",
    "db:studio": "npm run studio --workspace=@magimanager/database"
  },
  "devDependencies": {
    "turbo": "^2.3.0"
  },
  "packageManager": "npm@10.0.0"
}
